ultra_anova: ultra_anova.o imgio.o betai.o nrutil.o gammln.o betacf.o
	$(CC) $(EXTRAS) ultra_anova.o imgio.o betai.o nrutil.o gammln.o betacf.o \
	-o ultra_anova -lm
ultra_anova.o: ultra_anova.c
	$(CC) $(EXTRAS) -c ultra_anova.c
imgio.o: imgio.c
	$(CC) $(EXTRAS) -c imgio.c
betai.o: betai.c
	$(CC) $(EXTRAS) -c betai.c
nrutil.o: nrutil.c
	$(CC) $(EXTRAS) -c nrutil.c
gammln.o: gammln.c
	$(CC) $(EXTRAS) -c gammln.c
betacf.o: betacf.c
	$(CC) $(EXTRAS) -c betacf.c
